{
    "manifest_version": 3,
    "name": "N Bookmarks",
    "description": "N予備校の教材をブックマークしたい",
    "version": "0.1.0",
    "permissions": [
        "storage"
    ],
    "content_scripts": [
        {"matches": ["https://www.nnn.ed.nico/my_course"], "js": ["get.js"]},
        {"matches": ["https://www.nnn.ed.nico/courses/*/chapters/*"], "js": ["add.js"]}
    ]
}